{
    "manifest_version": 2,
    "name": "Generate form",
    "version": "1.0",

    "description": "Form generator",
    "icons": {
        "48": "icons/text.png"
    },

    "permissions": [
        "activeTab"
    ],
  
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content-scripts/content-script.js" 
            ],
            "run_at" : "document_end"
        }
    ]
}